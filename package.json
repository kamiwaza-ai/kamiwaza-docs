{
  "name": "kamiwaza-docs",
  "version": "0.7.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "cd docs && docusaurus start",
    "build:docs": "cd docs && docusaurus build",
    "build": "npm run sync-sdk && npm run build:docs",
    "swizzle": "docusaurus swizzle",
    "deploy": "cd docs && docusaurus deploy",
    "clear": "cd docs && docusaurus clear",
    "serve": "cd docs && docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "sync-sdk": "node -r ts-node/register scripts/sync-sdk-docs.ts",
    "version-docs": "node -r ts-node/register scripts/version.ts",
    "version-up": "node -r ts-node/register scripts/version-up.ts",
    "pdf": "node -r ts-node/register scripts/generate-pdf.ts",
    "pdf:offline": "npm run pdf -- --profile offline-install",
    "pdf:full": "npm run pdf -- --profile full-docs"
  },
  "dependencies": {
    "@docusaurus/core": "^3.8.1",
    "@docusaurus/plugin-content-blog": "^3.8.1",
    "@docusaurus/preset-classic": "^3.8.1",
    "@docusaurus/theme-classic": "^3.8.1",
    "@docusaurus/theme-mermaid": "^3.8.1",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.8.1",
    "@docusaurus/tsconfig": "^3.8.1",
    "@docusaurus/types": "^3.1.0",
    "@types/fs-extra": "^11.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/puppeteer": "^7.0.4",
    "fs-extra": "^11.0.0",
    "gh-pages": "^6.2.0",
    "js-yaml": "^4.1.0",
    "pdf-lib": "^1.17.1",
    "puppeteer": "^23.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
